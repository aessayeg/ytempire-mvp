# Frontend Development Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000
VITE_WS_RECONNECT_INTERVAL=5000

# App Configuration
VITE_APP_NAME=YTEmpire
VITE_APP_VERSION=0.1.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=false

# External Services (Development Keys)
VITE_STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
VITE_GOOGLE_CLIENT_ID=xxxxxxxxxxxxx.apps.googleusercontent.com

# Limits
VITE_MAX_CHANNELS=5
VITE_MAX_VIDEOS_PER_DAY=10
VITE_MAX_VIDEO_DURATION=3600

# Cost Limits
VITE_MAX_COST_PER_VIDEO=3.0
VITE_COST_WARNING_THRESHOLD=2.5