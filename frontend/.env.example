# Frontend Environment Variables
# Copy this file to .env and update with your values

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_DELAY=3000

# Authentication
VITE_AUTH_TOKEN_KEY=ytempire_auth_token
VITE_AUTH_REFRESH_KEY=ytempire_refresh_token
VITE_AUTH_PERSIST=true

# YouTube Configuration
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here
VITE_YOUTUBE_CLIENT_ID=your_youtube_client_id_here
VITE_YOUTUBE_REDIRECT_URI=http://localhost:3000/auth/youtube/callback

# Analytics
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=
VITE_ENABLE_ANALYTICS=false

# Feature Flags
VITE_ENABLE_BETA_FEATURES=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_NOTIFICATIONS=true

# Upload Configuration
VITE_MAX_UPLOAD_SIZE=104857600
VITE_ALLOWED_FILE_TYPES=.mp4,.mov,.avi,.mkv,.jpg,.jpeg,.png,.gif
VITE_CHUNK_SIZE=5242880

# UI Configuration
VITE_APP_NAME=YTEmpire
VITE_APP_TAGLINE=AI-Powered YouTube Content Automation
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_DEFAULT_LANGUAGE=en

# Performance
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_PWA=false
VITE_CACHE_TTL=3600000

# Payment/Stripe (if using)
VITE_STRIPE_PUBLIC_KEY=
VITE_ENABLE_PAYMENTS=false

# Sentry Error Tracking (optional)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Development
VITE_DEV_SERVER_PORT=3000
VITE_DEV_PROXY_TARGET=http://localhost:8000
VITE_ENABLE_HOT_RELOAD=true

# N8N Integration
VITE_N8N_WEBHOOK_URL=http://localhost:5678/webhook
VITE_N8N_API_KEY=

# Video Generation Settings
VITE_DEFAULT_VIDEO_QUALITY=1080p
VITE_DEFAULT_VIDEO_FPS=30
VITE_DEFAULT_VIDEO_FORMAT=mp4

# Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_WINDOW=60000

# Beta Features
VITE_BETA_SIGNUP_ENABLED=true
VITE_BETA_MAX_USERS=100

# Environment
NODE_ENV=development
VITE_APP_ENV=development